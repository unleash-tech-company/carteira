{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn run db:push && next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:push:test": "dotenv -e .env drizzle-kit push --url=$DATABASE_URL --dialect=postgresql --schema=./src/server/db/schema.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "e2e": "NODE_ENV=test dotenv -e .env -- playwright test",
    "e2e:ui": "NODE_ENV=test dotenv -e .env -- playwright test --ui",
    "tscheck": "tsc --noEmit"
  },
