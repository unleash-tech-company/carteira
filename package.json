{
  "name": "my-react-router-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "concurrently \"react-router dev\" \"npm run dev:zero-cache > /dev/null 2>&1\"",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down",
    "dev:clean": "source .env && docker volume rm -f docker_zstart_pgdata && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "dev:zero-cache": "zero-cache-dev -p app/db/schema.ts"
  },
  "dependencies": {
    "@clerk/react-router": "^1.1.0",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@rocicorp/rails": "^0.11.1",
    "@rocicorp/reflect": "^0.39.202402230127",
    "@rocicorp/zero": "^0.15.2025021402",
    "@types/js-cookie": "^3.0.6",
    "class-variance-authority": "^0.7.1",
    "drizzle": "^1.4.0",
    "drizzle-zero": "^0.5.0",
    "framer-motion": "^12.4.3",
    "isbot": "^5.1.17",
    "jose": "^5.10.0",
    "js-cookie": "^3.0.5",
    "js-cookies": "^1.0.4",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.5",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.2",
    "react-router-devtools": "^1.1.0",
    "tailwindcss": "3.4.17",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ]
}
